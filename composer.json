{
    "name": "mtahv3/laravel-queue-snssqs",
    "description": "Provides ability to handle Laravel Queues from a fan-out SNS/SQS pattern",
    "type": "laravel-package",
    "require": {
        "php": "^7.1.3",
        "illuminate/database": "5.8.*",
        "illuminate/support": "5.8.*",
        "illuminate/queue": "5.8.*",
        "aws/aws-sdk-php": "~3.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Mtahv3\\LaravelQueueSnsSqs\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mtahv3\\LaravelQueueSnsSqs\\LaravelQueueSnsSqsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}